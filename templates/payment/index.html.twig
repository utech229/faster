                                    <div class="d-flex align-items-center py-2">
                                    {% if (pDelete and pDelete == true) and app.user.paymentAccount != [] and app.user.paymentAccount[0]['Owner'] %}
                                        <button type="reset" data-id="momo" class="btn btn-sm btn-light btn-active-light-primary delete-payment-method-momo-button me-3">{% trans %}Supprimer{% endtrans %}</button>
                                    {% endif %}

                                    {% if (pCreate and pCreate == true) or (pEdit and  pEdit == true)  %}
                                        <button class="btn btn-sm btn-light btn-active-light-primary" data-bs-toggle="modal" data-bs-target="#kt_modal_momo">
                                        {% if app.user.paymentAccount != [] and app.user.paymentAccount[0]['Owner']%}
                                                {% trans %}Modifier{% endtrans %}
                                        {% else %}
                                                {% trans %}Ajouter{% endtrans %}
                                        {% endif %}
                                        </button>
                                    {% endif %}
                                    </div>
                                    <!--end::Actions-->
                                </div>
                                <!--end::Card-->
                            </div>
