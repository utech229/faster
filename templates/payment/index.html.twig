                            </button>
                            <!--end::Close-->
                        </div>
                        <!--end::Alert-->
                    </div>
                    <!--end::Row-->
                </div>
                <!--end::Card body-->
                {% endif %}
            </div>
            <!--end::Hero card-->

            <!--begin::Card-->
            <div class="card" id="payment_section_2">
