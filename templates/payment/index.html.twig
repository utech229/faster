                                <div class="px-7 py-5">
                                    <div class="fs-5 text-dark fw-bolder">{% trans %}Option de filtre{% endtrans %}</div>
                                </div>
                                <!--end::Header-->
                                <!--begin::Separator-->
                                <div class="separator border-gray-200"></div>
                                <!--end::Separator-->
                                <!--begin::Content-->
                                <div class="px-7 py-5" data-kt-payment-table-filter="form">
