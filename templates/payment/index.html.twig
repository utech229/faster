                                    <div class="m-0">
                                        <i class="bi bi-cash-coin fs-2x text-warning"></i>
                                    </div>
                                    <!--end::Icon-->
                            <span class="pulse-ring"></span>
                            </span>
                        </span>
                        <div class="d-flex flex-column text-end">
                            <span class="text-dark fw-bolder fs-2" id="stat_pending">
                            0
                            </span>
