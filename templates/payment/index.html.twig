                                    <div class="mb-10">
                                        <label class="form-label fs-6 fw-bold">{{'Marque'|trans}}:</label>
                                        <select class="form-select form-select-solid fw-bolder" data-kt-select2="true" data-placeholder="{{"Choisissez la marque"}}" data-allow-clear="true" id="brand_filter" data-kt-payment-table-filter="brand" data-hide-search="true">>
                                            <option value="">{{"Toutes"|trans}}</option>
                                            {% for brand in brands %}
                                                <option value="{{ brand.name }}" data-select2-avatar="{{brand.logo}}" data-select2-alt="{{brand.name}}">{{ brand.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                    <!--end::Input group-->#}
                                    <!--begin::Input group-->
                                    <div class="mb-10">
                                        <label class="form-label fs-6 fw-bold">{{'Status'|trans}}:</label>
                                        <select class="form-select form-select-solid fw-bolder" data-kt-select2="true" data-placeholder="{{"Choisissez le status"}}" data-allow-clear="true" id="status_filter" data-kt-payment-table-filter="status" data-hide-search="true">>
                                            <option value="">{{"Toutes"|trans}}</option>
                                            {% for status_one in status %}
                                                <option value="{{ status_one.name }}" data-select2-alt="{{brand.name}}">{{ status_one.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                    <!--end::Input group-->
                                    <!--begin::Actions-->
                                    <div class="d-flex justify-content-end">
                                        <button type="reset" class="btn btn-light btn-active-light-primary fw-bold me-2 px-6" data-kt-menu-dismiss="true" data-kt-payment-table-filter="reset">{% trans %}Remise à zéro{% endtrans %}</button>
                                        <button type="submit" class="btn btn-primary fw-bold px-6" data-kt-menu-dismiss="true" data-kt-payment-table-filter="filter">{% trans %}Filtrer{% endtrans %}</button>
                                    </div>
                                    <!--end::Actions-->
                                </div>
                                <!--end::Content-->
                            </div>
                            <!--end::Menu 1-->
                            <!--end::Filter-->

                            <!--begin::Add request-->
                            <button class="btn btn-primary"
                            {% if app.user and app.user.brand.commission <= 0 %}
                                disabled ="disabled"
                            {% endif %} 
                            {% if app.user.paymentAccount[0]['Status'] and app.user.paymentAccount[0]['Status'] != 'approved' %}
                                disabled="disabled"
                            {% endif %}  
                            data-bs-toggle="modal" data-bs-target="#kt_modal_payment_request">
                            <!--begin::Svg Icon | path: icons/duotune/arrows/arr075.svg-->
                            <span class="svg-icon svg-icon-2">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                    <rect opacity="0.5" x="11.364" y="20.364" width="16" height="2" rx="1" transform="rotate(-90 11.364 20.364)" fill="currentColor" />
                                    <rect x="4.36396" y="11.364" width="16" height="2" rx="1" fill="currentColor" />
                                </svg>
                            </span>
                            <!--end::Svg Icon-->{{'Demande de retrait'| trans}}</button>

                            <button type="button" class="btn btn-warning d-none" id="kt_modal_payment_request_treat_button" data-bs-toggle="modal" data-bs-target="#kt_modal_payment_request_treat">
                                <span class="svg-icon svg-icon-2">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                        <rect opacity="0.5" x="11.364" y="20.364" width="16" height="2" rx="1" transform="rotate(-90 11.364 20.364)" fill="currentColor" />
                                        <rect x="4.36396" y="11.364" width="16" height="2" rx="1" fill="currentColor" />
                                    </svg>
                                </span>
                                {{'Traiter une demande'| trans}}
                            </button>

                            <button type="button" id="kt_modal_add_payment_reload_button" class="btn btn-light-primary d-none">
                            <!--begin::Svg Icon | path: icons/duotune/general/gen035.svg-->
                            <span class="svg-icon svg-icon-3">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                    <rect opacity="0.3" x="2" y="2" width="20" height="20" rx="5" fill="black" />
                                    <rect x="10.8891" y="17.8033" width="12" height="2" rx="1" transform="rotate(-90 10.8891 17.8033)" fill="black" />
                                    <rect x="6.01041" y="10.9247" width="12" height="2" rx="1" fill="black" />
                                </svg>
                            </span>
                            <!--end::Svg Icon-->{% trans %}Nouvelle{% endtrans %}</button>
                            <!--end::Add subscription-->
                        </div>
                        <!--end::Toolbar-->
                        <!--begin::Group actions-->
                        <div class="d-flex justify-content-end align-items-center d-none" data-kt-payment-table-toolbar="selected">
                            <div class="fw-bolder me-5">
