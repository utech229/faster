                                    <div class="d-flex flex-column py-2">
                                        <!--begin::Owner-->
                                        <div class="d-flex align-items-center fs-4 fw-bolder mb-5">{{'Mobile money'|upper|trans}}
                                        {% if app.user.paymentAccount != []  %}
                                            {% if app.user.paymentAccount[0]['Status'] == 'approved' %}
                                            <span class="badge badge-light-success fs-7 ms-2">
                                            {% trans %}Actif{% endtrans %}
                                            </span>
                                            {% else %}
                                            <span class="badge badge-light-danger fs-7 ms-2">
                                            {% trans %}Non vérifié{% endtrans %}
                                            </span>
                                            {% endif %}
                                        {% else %}
                                            <span class="badge badge-light-warning fs-7 ms-2">
                                            {% trans %}Non configuré{% endtrans %}
                                            </span>
                                        {% endif %}
                                        </div>
                                        <!--end::Owner-->
                                        <!--begin::Wrapper-->
                                        <div class="d-flex align-items-center">
                                            <!--begin::Icon-->
                                            <img src="{{ asset('app/media/svg/card-logos/mobile.svg') }}" alt="" width="75" class="me-4" />
                                            <!--end::Icon-->
                                            <!--begin::Details-->
                                            <div>
                                                {% if app.user.paymentAccount != [] and app.user.paymentAccount[0]['Owner'] %}
                                                    {% if  app.user.paymentAccount[0]['Status'] == 'approved' %}
                                                    <div class="fs-4 fw-bolder">{{ app.user.paymentAccount[0]['Operator'] }}</div>
                                                    <div class="fs-6 fw-bold text-gray-400">{{ app.user.paymentAccount[0]['Phone'] }}</div>
                                                    {% else %}
                                                     <div class="fs-4 fw-bolder">{{ app.user.paymentAccount[0]['newOperator'] }}</div>
                                                    <div class="fs-6 fw-bold text-gray-400">{{ app.user.paymentAccount[0]['newPhone'] }}</div>
                                                    {% endif %}
                                                {% else %}
                                                <div class="d-flex align-items-center  text-danger py-2">
                                                <span class="bullet bg-danger me-3"></span>{% trans %}Méthode non configuré{% endtrans %}</div>
                                                {% endif %}
                                            </div>
                                            <!--end::Details-->
                                        </div>
                                        <!--end::Wrapper-->
                                    </div>
                                    <!--end::Info-->
