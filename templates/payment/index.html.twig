                            <button type="button" class="btn btn-danger" data-kt-payment-table-select="delete_selected">Delete Selected</button>
                        </div>
                        <!--end::Group actions-->
                    </div>
                    <!--end::Card toolbar-->
                </div>
                <!--end::Card header-->
                <!--begin::Card body-->
                <div class="card-body pt-0">
                    <!--begin::Table-->
                    <table class="table align-middle table-row-dashed fs-6 gy-5" id="kt_payments_table">
                        <!--begin::Table head-->
                        <thead>
                            <!--begin::Table row-->
                            <tr class="text-start text-muted fw-bolder fs-7 text-uppercase gs-0">
                                <th class="w-10px pe-2">
                                    <div class="form-check form-check-sm form-check-custom form-check-solid me-3">
                                        <input class="form-check-input" type="checkbox" data-kt-check="true" data-kt-check-target="#kt_payments_table .form-check-input" value="1" />
                                    </div>
                                </th>
                                <th class="min-w-125px th-table">{% trans %}Référence{% endtrans %}</th>
                                <th class="min-w-125px th-table">{% trans %}Id de transaction{% endtrans %}</th>
                                <th class="min-w-125px th-table">{% trans %}Demandé par{% endtrans %}</th>
                                <th class="min-w-125px th-table">{% trans %}Méthode{% endtrans %}</th>
                                <th class="min-w-125px th-table">{% trans %}Montant{% endtrans %}</th>
                                <th class="min-w-125px th-table">{% trans %}Valider par{% endtrans %}</th>
                                <th class="min-w-125px th-table">{% trans %}Demandé le{% endtrans %}</th>
                                <th class="min-w-125px th-table">{% trans %}Statut{% endtrans %}</th>
                                <th class="min-w-125px th-table text-center">{% trans %}Action{% endtrans %}</th>
                            </tr>
                            <!--end::Table row-->
                        </thead>
                        <!--end::Table head-->
                        <!--begin::Table body-->
                        <tbody class="text-gray-600 fw-bold">
                            
                        </tbody>
                        <!--end::Table body-->
                    </table>
                    <!--end::Table-->
                </div>
                <!--end::Card body-->
            </div>
            <!--end::Card-->
            <!--begin::Payment methods-->
            <div class="card mb-5 mb-xl-10 d-none" id="payment_methode">
