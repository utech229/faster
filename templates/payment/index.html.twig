                <div class="card-body">
                    <!--begin::Row-->
                    <div class="row">
                         <!--begin::Alert-->
                        <div class="alert alert-dismissible bg-light-danger border border-danger border-dashed  h-lg-100 d-flex flex-column flex-sm-row w-100 p-5">
