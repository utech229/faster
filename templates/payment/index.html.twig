                        </ul>
                        <!--end::Nav-->
                    </div>
                    <!--end::Hero nav-->
                </div>
                <!--end::Hero body-->
                {% if app.user.paymentAccount == [] or app.user.paymentAccount[0]['Status'] is defined and app.user.paymentAccount[0]['Status'] != 'approved' %}
